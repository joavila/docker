/var/log/err
/var/log/out
{
  daily
  compress
  missingok
  copytruncate
  rotate 1
  lastaction
    case ${BUCKET} in
      "") echo No bucket available;;
      *) cd /var/log && for file in $(ls -1 *.gz | xargs); do
           curl --silent --show-error --fail --upload-file ${file} ${BUCKET} && rm ${file};
         done;;
    esac
  endscript
}
